[Keyboard]
# The config values for keyboard navigation. 
# By default, we use vi keybindings.
down=j
up=k
next=n
previous=p
quit=q
top=g
bottom=G
toggle-read=r
mark-all-read=R

[Posts]
# The maximum width for wrapping the posts
# If this is greater than the terminal width, 
# words will be wrapped to match the terminal instead.
max-width=80
# Key to use to extract a list of all links within a post
# The links can then be opened in the system browser
extract-links=L
